#! /bin/sh

# Import the WN database and convert it into a SQLITE database where possible.

# The .tar.gz file is expected on output.

mkdir /tmp/wn

cd /tmp/wn

tar xzf /dev/stdin

#wnimport-exe ./dict/index.adv /b -t adverb -m idxtest
#wnimport-exe ./dict/index.adj /b -t adject -m idxtest
wnimport-exe ./dict/index.noun /b -t noun -m idxtest
wnimport-exe ./dict/index.verb /b -t verb -m idxtest

